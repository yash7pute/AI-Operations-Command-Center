{
  "name": "my-node-ts-app",
  "version": "0.1.0",
  "private": true,
  "description": "Node.js + TypeScript starter with integrations for Gmail, Slack, Notion, and Google APIs",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest --runInBand --detectOpenHandles",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "demo": "ts-node demo/reasoning-demo.ts",
    "demo:build": "tsc demo/reasoning-demo.ts --outDir dist/demo && node dist/demo/reasoning-demo.js",
    "benchmark": "ts-node tests/performance/reasoning-benchmarks.ts",
    "benchmark:build": "tsc tests/performance/reasoning-benchmarks.ts --outDir dist/benchmarks && node dist/benchmarks/reasoning-benchmarks.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@notionhq/client": "^3.0.0",
    "@slack/bolt": "^3.12.0",
    "axios": "^1.5.0",
    "dotenv": "^16.3.1",
    "googleapis": "^121.0.0",
    "groq-sdk": "^0.33.0",
    "openai": "^6.3.0",
    "tiktoken": "^1.0.22",
    "together-ai": "^0.22.0",
    "winston": "^3.9.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/node": "^20.4.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.0"
  }
}
